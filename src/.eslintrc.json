{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
    },
    "extends": [
      "standard",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2021
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "semi": ["error", "always"],
      "indent": ["error", 2]
    }
  }
  